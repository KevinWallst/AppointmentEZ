{
  "name": "appointmentez",
  "version": "0.2.0",
  "description": "\"AppointmentEZ - A simple and elegant appointment booking system\"",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:3000": "cross-env PORT=3000 next dev",
    "build": "next build",
    "start": "next start",
    "start:3000": "cross-env PORT=3000 next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:timezone": "jest __tests__/utils/dateUtils.test.ts",
    "test:imports": "jest __tests__/imports",
    "validate-imports": "node scripts/validate-imports.js",
    "prepare": "husky install",
    "restart": "node restart-server-port-3000.js",
    "render-build": "npm install && npm run build && npm run validate-imports",
    "render-start": "npm run start",
    "build:commit": "chmod +x scripts/build-and-commit.sh && ./scripts/build-and-commit.sh",
    "build:version": "chmod +x scripts/post-build.sh && ./scripts/post-build.sh",
    "update:changelog": "chmod +x scripts/pre-build.sh && ./scripts/pre-build.sh",
    "master-build": "chmod +x scripts/master-build.sh && ./scripts/master-build.sh"
  },
  "keywords": [
    "\"calendar\"",
    "\"booking\"",
    "\"appointment\"",
    "\"scheduling\"",
    "\"timeshlots\"",
    "\"react\"",
    "\"next.js\""
  ],
  "author": "Kevin Zhu",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "pwd"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "data-fns": "latest",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "fast-csv": "latest",
    "next": "latest",
    "nodemailer": "^6.10.1",
    "react": "latest",
    "react-dom": "latest",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.14.1",
    "@types/react": "19.1.1",
    "cross-env": "^7.0.3",
    "glob": "^10.4.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "5.8.3"
  }
