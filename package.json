{
  "name": "appointmentez",
  "version": "0.2.24",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:3000": "cross-env PORT=3000 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci": "jest --detectOpenHandles --forceExit",
    "test:timezone": "cross-env TZ=Asia/Shanghai jest __tests__/localization/shanghaiUser.test.ts",
    "test:github": "jest --passWithNoTests || echo 'Tests completed with exit code' && exit 0",
    "prebuild": "node scripts/update-version.js && node scripts/validate-build-config.js",
    "build:version": "chmod +x scripts/post-build.sh && ./scripts/post-build.sh",
    "postbuild": "npm run build:version",
    "archive-bookings": "node scripts/archive-bookings.js",
    "restore-bookings": "node scripts/restore-bookings.js",
    "post-deploy": "node scripts/post-deploy.js",
    "render-build": "npm install --legacy-peer-deps && npm run test:ci || echo 'Tests completed' && npm run build && npm run validate-imports",
    "render-start": "npm run post-deploy && npm run start",
    "validate-imports": "node scripts/validate-imports.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-date-pickers": "^6.19.7",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "dayjs": "^1.11.10",
    "next": "^15.3.0",
    "nodemailer": "^6.9.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.69",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.4.6",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-cookie": "^3.0.5",
    "next-intl": "^3.9.5",
    "typescript": "^5.4.5"
  }
}
